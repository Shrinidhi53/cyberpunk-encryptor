{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "SECRET_KEY": "@secret_key",
    "FLASK_DEBUG": "False"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  }
}
